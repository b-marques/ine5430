############################
#   Classes
############################

# Class: :MachineGun (:MachineGun)

SubClassOf(:MachineGun :gun)
DisjointClasses(:MachineGun :pistol)
DisjointClasses(:MachineGun :rifle)

# Class: :apc (:apc)

SubClassOf(:apc :vehicle)

# Class: :attack_helicopter (:attack_helicopter)

SubClassOf(:attack_helicopter :helicotper)

# Class: :boat (:boat)

SubClassOf(:boat :vehicle)

# Class: :car (:car)

SubClassOf(:car :vehicle)

# Class: :china (:china)

SubClassOf(:china :team)

# Class: :conservative_soldier (:conservative_soldier)

SubClassOf(:conservative_soldier :soldier)
SubClassOf(:conservative_soldier ObjectAllValuesFrom(:kills :men))
DisjointClasses(:conservative_soldier :crazy_soldier)
DisjointClasses(:conservative_soldier :normal_soldier)
DisjointClasses(:conservative_soldier :peaceful_soldier)

# Class: :crazy_soldier (:crazy_soldier)

SubClassOf(:crazy_soldier :soldier)
SubClassOf(:crazy_soldier ObjectSomeValuesFrom(:kills :human))
SubClassOf(:crazy_soldier ObjectMinCardinality(3 :uses_gun :gun))
DisjointClasses(:crazy_soldier :normal_soldier)
DisjointClasses(:crazy_soldier :peaceful_soldier)

# Class: :driver (:driver)

SubClassOf(:driver :soldier_function)

# Class: :eu (:eu)

SubClassOf(:eu :team)

# Class: :helicotper (:helicotper)

SubClassOf(:helicotper :vehicle)

# Class: :infantry (:infantry)

SubClassOf(:infantry :soldier_function)

# Class: :jet (:jet)

SubClassOf(:jet :vehicle)

# Class: :mec (:mec)

SubClassOf(:mec :team)

# Class: :men (:men)

SubClassOf(:men :human)
DisjointClasses(:men :women)

# Class: :non_soldier_spirit (:non_soldier_spirit)

SubClassOf(:non_soldier_spirit :spirit)
DisjointClasses(:non_soldier_spirit :soldier_spirit)

# Class: :normal_soldier (:normal_soldier)

SubClassOf(:normal_soldier :soldier)
SubClassOf(:normal_soldier ObjectSomeValuesFrom(:kills :human))
SubClassOf(:normal_soldier ObjectAllValuesFrom(:uses_gun :pistol))
DisjointClasses(:normal_soldier :peaceful_soldier)

# Class: :peaceful_soldier (:peaceful_soldier)

SubClassOf(:peaceful_soldier :soldier)
SubClassOf(:peaceful_soldier ObjectComplementOf(ObjectSomeValuesFrom(:kills :human)))
SubClassOf(:peaceful_soldier ObjectComplementOf(ObjectSomeValuesFrom(:uses_gun :gun)))

# Class: :pilot (:pilot)

SubClassOf(:pilot :soldier_function)

# Class: :pistol (:pistol)

SubClassOf(:pistol :gun)
DisjointClasses(:pistol :rifle)

# Class: :rifle (:rifle)

SubClassOf(:rifle :gun)

# Class: :soldier (:soldier)

EquivalentClasses(:soldier ObjectIntersectionOf(:men ObjectSomeValuesFrom(:has_soldier_spirit :soldier_spirit)))

# Class: :soldier_spirit (:soldier_spirit)

SubClassOf(:soldier_spirit :spirit)

# Class: :tank (:tank)

SubClassOf(:tank :vehicle)

# Class: :transport_helicopter (:transport_helicopter)

SubClassOf(:transport_helicopter :helicotper)

# Class: :usmc (:usmc)

SubClassOf(:usmc :team)

# Class: :women (:women)

SubClassOf(:women :human)


