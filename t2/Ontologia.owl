ClassAssertion(:woman :Alessandra)

# Individual: :Bob (:Bob)

ClassAssertion(:normal_soldier :Bob)
ObjectPropertyAssertion(:drives :Bob :APC)
ObjectPropertyAssertion(:fights_for :Bob :mec_team)
ObjectPropertyAssertion(:uses_gun :Bob :USP)

# Individual: :CRAZYYYYY (:CRAZYYYYY)

ClassAssertion(:crazy_soldier :CRAZYYYYY)
ObjectPropertyAssertion(:uses_gun :CRAZYYYYY :Ak-47)
ObjectPropertyAssertion(:uses_gun :CRAZYYYYY :Negev)
ObjectPropertyAssertion(:uses_gun :CRAZYYYYY :USP)

# Individual: :Fabio_Conservador (:Fabio_Conservador)

ClassAssertion(:conservative_soldier :Fabio_Conservador)

# Individual: :MenWIthoutSpirit (:MenWIthoutSpirit)

ClassAssertion(:man :MenWIthoutSpirit)

# Individual: :MenWithSpirit (:MenWithSpirit)

ClassAssertion(:man :MenWithSpirit)
ObjectPropertyAssertion(:has_soldier_spirit :MenWithSpirit :SoldierSpirit)

# Individual: :Negev (:Negev)

ClassAssertion(:MachineGun :Negev)

# Individual: :Player_Da_Paz (:Player_Da_Paz)

ClassAssertion(:peaceful_soldier :Player_Da_Paz)

# Individual: :Ronald (:Ronald)

ClassAssertion(:normal_soldier :Ronald)

# Individual: :SoldierSpirit (:SoldierSpirit)

ClassAssertion(:soldier_spirit :SoldierSpirit)

# Individual: :USP (:USP)

ClassAssertion(:pistol :USP)

# Individual: :mec_team (:mec_team)

ClassAssertion(:mec :mec_team)


DisjointClasses(:apc :boat :car :helicotper :jet :tank)
DisjointClasses(:china :eu :mec :usmc)
DisjointClasses(:gun :human :spirit :team :terrain :vehicle)
DifferentIndividuals(:Bob :Ronald)
